<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.panll.assist.dao.DeviceChannelMapper">

    <select id="selectByStreamId" parameterType="java.lang.String" resultMap="BaseResultMap">
        select <include refid="Base_Column_List"/>
        from device_channel
        where streamId = #{streamId,jdbcType=VARCHAR}
    </select>

</mapper>